{
  "name": "lifecalling",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "pnpm --filter ./apps/web dev",
    "dev:web:turbo": "pnpm --filter ./apps/web dev:turbo",
    "dev:web:webpack": "pnpm --filter ./apps/web dev:webpack",
    "dev:api": "docker compose up -d && docker compose logs -f api",
    "storybook": "pnpm --filter ./apps/web storybook",
    "types:gen": "pnpm --filter @lifecalling/types gen && pnpm --filter @lifecalling/types build",
    "build": "pnpm --filter ./apps/web build",
    "lint": "pnpm --filter ./apps/web lint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@storybook/addon-a11y": "9.1.8",
    "@storybook/addon-themes": "^9.1.8",
    "@storybook/nextjs": "9.1.8",
    "openapi-typescript": "^7.9.1",
    "storybook": "^9.1.8"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "lucide-react": "^0.544.0",
    "recharts": "^3.2.1"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
